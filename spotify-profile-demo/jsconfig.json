{
    "compilerOptions": {
        "checkJs": true,
        "module": "ES2022",
        "target": "ES2020",
        "strict": true,
        "noImplicitAny": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitOverride": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedParameters": true,
    }
}